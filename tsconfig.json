{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/404.js", "pages/blog.js", "pages/_app.js", "components/Layout/index.js", "components/Header/index.js", "components/Footer/index.js", "pages/index.js", "pages/users/index.js", "pages/users/[id].js"],
  "exclude": ["node_modules"]
}
